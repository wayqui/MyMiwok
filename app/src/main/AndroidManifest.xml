<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.mymiwok">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PhrasesActivity" android:label="@string/str_phrases"
            android:parentActivityName="com.example.android.mymiwok.MainActivity"/>
        <activity android:name=".NumbersActivity" android:label="@string/str_numbers"
            android:parentActivityName="com.example.android.mymiwok.MainActivity"/>
        <activity android:name=".FamilyActivity" android:label="@string/str_family"
            android:parentActivityName="com.example.android.mymiwok.MainActivity"/>
        <activity android:name=".ColorsActivity" android:label="@string/str_colors"
            android:parentActivityName="com.example.android.mymiwok.MainActivity"/>
    </application>

</manifest>